/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=notAandB);
    Nand(a=a, b=notAandB, out=intA);
    Nand(a=b, b=notAandB, out=intB);
    Nand(a=intA, b=intB, out=out);
}